  <process processType="Private" isExecutable="true" id="hello" name="HelloWorld" tns:packageName="org.jboss.qa.brms.jbpm.functional" tns:version="2.0" >

    <!-- nodes -->
    <startEvent id="_1" name="Start" />
    <scriptTask id="_2" name="Hello world" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Hello world!");</script>
    </scriptTask>
    <endEvent id="_3" name="End" >
        <terminateEventDefinition/>
    </endEvent>
    <intermediateCatchEvent id="_4" name="Timer" >
      <timerEventDefinition>
        <timeDuration xsi:type="tFormalExpression">500ms</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <parallelGateway id="_5" name="Converge" gatewayDirection="Converging" />
    <scriptTask id="_6" name="Goodbye world" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Goodbye world!");</script>
    </scriptTask>
    <parallelGateway id="_7" name="Diverge" gatewayDirection="Diverging" />

